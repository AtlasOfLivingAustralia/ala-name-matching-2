{
  "id": "simple-linnaean",
  "description": "A simple Linnaean heirarchy for test purposes",
  "concept": "http://rs.tdwg.org/dwc/terms/Taxon",
  "normalisers": [
    {
      "@class" : "au.org.ala.util.BasicNormaliser",
      "id" : "simple_normaliser",
      "normaliseSpaces" : true,
      "normalisePunctuation" : true,
      "normaliseSymbols" : true,
      "normaliseAccents" : true,
      "normaliseCase" : false
    },
    {
      "@class" : "au.org.ala.util.BasicNormaliser",
      "id" : "lower_case_normaliser",
      "normaliseSpaces" : true,
      "normalisePunctuation" : true,
      "normaliseSymbols" : true,
      "normaliseAccents" : true,
      "normaliseCase" : true
    }
  ],
  "observables": [
    {
      "id": "taxonID",
      "uri": "http://rs.tdwg.org/dwc/terms/taxonID",
      "properties": {
        "http://ala.org.au/bayesian/1.0/identifier": true
      }
    },
    {
      "id": "taxonRank",
      "uri": "http://rs.tdwg.org/dwc/terms/taxonRank",
      "normaliser": "lower_case_normaliser"
    },
    {
      "id": "scientificName",
      "uri": "http://rs.tdwg.org/dwc/terms/scientificName",
      "multiplicity" : "*",
      "properties": {
        "http://ala.org.au/bayesian/1.0/name": true,
        "http://ala.org.au/optimisation/1.0/analyse": true,
        "http://ala.org.au/optimisation/1.0/loadFromClass": [
          "http://rs.tdwg.org/dwc/terms/Taxon",
          "http://ala.org.au/terms/1.0/TaxonVariant"
        ]
      },
      "normaliser": "simple_normaliser"
    },
    {
      "id": "altScientificName",
      "uri": "http://ala.org.au/terms/1.0/altScientificName",
      "properties": {
        "http://ala.org.au/bayesian/1.0/altName": true
      }
    },
    {
      "id": "synonymScientificName",
      "uri": "http://ala.org.au/terms/1.0/synonymScientificName",
      "properties": {
        "http://ala.org.au/bayesian/1.0/synonymName": true
      },
      "multiplicity": "*"
    },
    {
      "id": "soundexScientificName",
      "uri": "http://ala.org.au/terms/1.0/soundexScientificName",
      "properties": {
        "http://ala.org.au/optimisation/1.0/load": false
      },
      "multiplicity" : "*",
      "derivation": {
        "@class": "au.org.ala.bayesian.derivation.SoundexDerivation",
        "sources": [ "scientificName" ]
      }
    },
    {
      "id": "specificEpithet",
      "uri": "http://rs.tdwg.org/dwc/terms/specificEpithet",
      "properties": {
        "http://ala.org.au/optimisation/1.0/analyse": true,
        "http://ala.org.au/optimisation/1.0/load": false
      },
      "normaliser": "simple_normaliser",
      "group": "species"
    },
    {
      "id": "genus",
      "uri": "http://rs.tdwg.org/dwc/terms/genus",
      "properties": {
        "http://ala.org.au/optimisation/1.0/analyse": true,
        "http://ala.org.au/optimisation/1.0/load": false,
        "http://ala.org.au/bayesian/1.0/copy": true
      },
      "normaliser": "simple_normaliser",
      "group": "genus",
      "base": {
        "@class": "au.org.ala.bayesian.derivation.ParentDerivation",
        "sources": [ "scientificName" ],
        "select" : {
          "@class": "au.org.ala.bayesian.condition.ValueCondition",
          "source": "taxonRank",
          "value": "genus"
        }
      }
    },
    {
      "id": "soundexGenus",
      "uri": "http://ala.org.au/terms/1.0/soundexGenus",
      "properties": {
        "http://ala.org.au/optimisation/1.0/load": false
      },
      "multiplicity" : "*",
      "derivation": {
        "@class": "au.org.ala.bayesian.derivation.SoundexDerivation",
        "sources": [ "genus" ]
      }
    },
    {
      "id": "family",
      "uri": "http://rs.tdwg.org/dwc/terms/family",
      "properties": {
        "http://ala.org.au/bayesian/1.0/copy": true,
        "http://ala.org.au/optimisation/1.0/loadAsVariant": true
      },
      "normaliser": "simple_normaliser",
      "multiplicity": "*",
      "base": {
        "@class": "au.org.ala.bayesian.derivation.ParentDerivation",
        "sources": [ "scientificName", "synonymScientificName" ],
        "select" : {
          "@class": "au.org.ala.bayesian.condition.ValueCondition",
          "source": "taxonRank",
          "value": "family"
        }
      }
    },
    {
      "id": "order",
      "uri": "http://rs.tdwg.org/dwc/terms/order",
      "properties": {
        "http://ala.org.au/bayesian/1.0/copy": true,
        "http://ala.org.au/optimisation/1.0/loadAsVariant": true
      },
      "normaliser": "simple_normaliser",
      "multiplicity": "*",
      "base": {
        "@class": "au.org.ala.bayesian.derivation.ParentDerivation",
        "sources": [ "scientificName", "synonymScientificName" ],
        "select" : {
          "@class": "au.org.ala.bayesian.condition.ValueCondition",
          "source": "taxonRank",
          "value": "order"
        }
      }
    },
    {
      "id": "class",
      "uri": "http://rs.tdwg.org/dwc/terms/class",
      "properties": {
        "http://ala.org.au/bayesian/1.0/copy": true,
        "http://ala.org.au/optimisation/1.0/loadAsVariant": true

      },
      "normaliser": "simple_normaliser",
      "multiplicity": "*",
      "base": {
        "@class": "au.org.ala.bayesian.derivation.ParentDerivation",
        "sources": [ "scientificName", "synonymScientificName" ],
        "select" : {
          "@class": "au.org.ala.bayesian.condition.ValueCondition",
          "source": "taxonRank",
          "value": "class"
        }
      }
    },
    {
      "id": "phylum",
      "uri": "http://rs.tdwg.org/dwc/terms/phylum",
      "properties": {
        "http://ala.org.au/bayesian/1.0/copy": true,
        "http://ala.org.au/optimisation/1.0/loadAsVariant": true
      },
      "normaliser": "simple_normaliser",
      "multiplicity": "*",
      "base": {
        "@class": "au.org.ala.bayesian.derivation.ParentDerivation",
        "sources": [ "scientificName", "synonymScientificName" ],
        "select" : {
          "@class": "au.org.ala.bayesian.condition.ValueCondition",
          "source": "taxonRank",
          "value": "phylum"
        }
      }
    },
    {
      "id": "kingdom",
      "uri": "http://rs.tdwg.org/dwc/terms/kingdom",
      "properties": {
        "http://ala.org.au/bayesian/1.0/copy": true,
        "http://ala.org.au/optimisation/1.0/load": false
      },
      "normaliser": "simple_normaliser",
      "multiplicity": "*",
      "base": {
        "@class": "au.org.ala.bayesian.derivation.ParentDerivation",
        "sources": [ "scientificName", "synonymScientificName" ],
        "select" : {
          "@class": "au.org.ala.bayesian.condition.ValueCondition",
          "source": "taxonRank",
          "value": "kingdom"
        }
      }
    },
    {
      "id": "scientificNameAuthorship",
      "uri": "http://rs.tdwg.org/dwc/terms/scientificNameAuthorship",
      "normaliser": "simple_normaliser"
    },
    {
      "id": "priority",
      "label": "p",
      "type": "java.lang.Integer",
      "uri": "http://ala.org.au/terms/1.0/priority",
      "properties": {
        "http://ala.org.au/optimisation/1.0/aggregate": "max",
        "http://ala.org.au/optimisation/1.0/loadFromClass": "http://ala.org.au/terms/1.0/TaxonVariant"
      }
    },
    {
      "id": "weight",
      "uri": "http://ala.org.au/bayesian/1.0/weight",
      "type": "java.lang.Double",
      "properties": {
        "http://ala.org.au/bayesian/1.0/weight": true
      }
    },
    {
      "id": "parentNameUsageID",
      "uri": "http://rs.tdwg.org/dwc/terms/parentNameUsageID",
      "properties": {
        "http://ala.org.au/bayesian/1.0/parent": true,
        "http://ala.org.au/bayesian/1.0/additional": true
      }
    },
    {
      "id": "acceptedNameUsageID",
      "uri": "http://rs.tdwg.org/dwc/terms/acceptedNameUsageID",
      "properties": {
        "http://ala.org.au/bayesian/1.0/accepted": true,
        "http://ala.org.au/bayesian/1.0/additional": true
     }
    },
    {
      "id": "taxonomicStatus",
      "uri": "http://rs.tdwg.org/dwc/terms/taxonomicStatus",
      "normaliser": "simple_normaliser",
      "properties": {
        "http://ala.org.au/bayesian/1.0/additional": true
      }
    }
  ],
  "vertices": [
    "taxonID",
    "taxonRank",
    "scientificName",
    "soundexScientificName",
    "specificEpithet",
    "genus",
    "family",
    "order",
    "class",
    "phylum",
    "kingdom",
    "scientificNameAuthorship"
  ],
  "edges": [
    { "source": "taxonID", "target": "scientificName",  "edge": { } },
    { "source": "taxonID", "target": "taxonRank",  "edge": { } },
    { "source": "taxonID", "target": "specificEpithet",  "edge": { } },
    { "source": "taxonID", "target": "scientificNameAuthorship",  "edge": { } },
    { "source": "specificEpithet", "target": "scientificName",  "edge": { } },
    { "source": "scientificName", "target": "genus",  "edge": { } },
    { "source": "scientificName", "target": "soundexScientificName",  "edge": { } },
    { "source": "soundexScientificName", "target": "genus",  "edge": { } },
    { "source": "genus", "target": "family",  "edge": { } },
    { "source": "family", "target": "order",  "edge": { } },
    { "source": "order", "target": "class",  "edge": { } },
    { "source": "class", "target": "phylum",  "edge": { } },
    { "source": "phylum", "target": "kingdom",  "edge": { } }
  ],
  "issues": [
    {
      "id": "removed_rank",
      "uri": "http://ala.org.au/issues/1.0/removedRank",
      "description": "Ignored supplied rank to find a match."
    },
    {
      "id": "removed_phylum",
      "uri": "http://ala.org.au/issues/1.0/removedPhylum",
      "description": "Ignored supplied phylum to find a match."
    },
    {
      "id": "removed_class",
      "uri": "http://ala.org.au/issues/1.0/removedClass",
      "description": "Ignored supplied class to find a match."
    },
    {
      "id": "removed_order",
      "uri": "http://ala.org.au/issues/1.0/removedOrder",
      "description": "Ignored supplied order to find a match."
    }
  ],
  "modifications": [
    {
      "@class" : "au.org.ala.bayesian.modifier.RemoveModifier",
      "id" : "remove_rank",
      "issues" : [ "removed_rank" ],
      "observables" : [ "taxonRank" ]
    },
    {
      "@class" : "au.org.ala.bayesian.modifier.RemoveModifier",
      "id" : "remove_order",
      "issues" : [ "removed_order" ],
      "observables" : [ "order" ]
    },
    {
      "@class" : "au.org.ala.bayesian.modifier.RemoveModifier",
      "id" : "remove_class",
      "issues" : [ "removed_class" ],
      "observables" : [ "class" ]
    },
    {
      "@class" : "au.org.ala.bayesian.modifier.RemoveModifier",
      "id" : "remove_phylum",
      "issues" : [ "removed_phylum" ],
      "condition" : {
        "@class" : "au.org.ala.bayesian.condition.PresentCondition",
        "all": false,
        "present": [
          "class",
          "order",
          "family"
        ]
      },
      "observables" : [ "phylum" ]
    }
  ],
  "matchModifiers": [
    [
      "remove_order", "remove_class", "remove_phylum"
    ],
    [
      "remove_rank"
    ]

  ]
}